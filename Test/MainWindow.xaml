                <GxControls:HintSearchBox Name="TxtSeatchSmall" Height="30" HintText="Buscar más" VerticalContentAlignment="Center" Margin="5" KeyDown="TxtSeatch_KeyDown"/>                  
            </StackPanel>
        </TabItem>
        <TabItem Header="SVG">
            <Grid>
                <ContentControl Template="{StaticResource GnuIcon}" />
            </Grid>
        </TabItem>
        <TabItem Header="Buttons">
            <StackPanel>
                <Button Width="250" Padding="8" Height="60">
                    <DockPanel>
                        <ContentControl Template="{StaticResource GnuIcon}" DockPanel.Dock="Right"/>
                        <TextBlock Text="Botón con icono" VerticalAlignment="Center" Margin="20,0" />
                    </DockPanel>
                </Button>                
            </StackPanel>
        </TabItem>
        <TabItem Header="Animated">
            <StackPanel>
                <GxControls:ProgressRing x:Name="RingTest" Width="50" Height="50" IsActivate="True"/>
                <ToggleButton Click="ToggleButton_Click" Content="activate/desactivate"/>
            </StackPanel>
        </TabItem>
    </TabControl>
</Window>
